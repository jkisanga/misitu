@model Misitu.Applicants.CreateForestProduceRegistration
@{
    ViewBag.Title = "Forest Produce Registration";
}
<div class="panel panel-info">
    <div class="panel-heading">
        <div class="pull-left"><span>Forest Produce Registration </span></div>
        <div class="clearfix"></div>
    </div>
    <div class="panel-body">
        <div class="padd">
