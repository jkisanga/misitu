@using ReportViewerForMvc
@{
    ViewBag.Title = "RegCertViewer";
}


@{
    ViewBag.ActiveMenu = "Registration";
    ViewBag.Title = "Registration";
}

<div class="widget">
    <div class="widget-head">
        <div class="pull-left">Certificate of Registration</div>
        <div class="clearfix"></div>
    </div>
    <div class="widget-content">
        <div class="padd">
            @Html.ReportViewer(ViewBag.rptRegCert as Microsoft.Reporting.WebForms.ReportViewer)
        </div>
    </div>
</div>






