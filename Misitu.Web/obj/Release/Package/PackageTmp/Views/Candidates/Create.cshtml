
@{
    ViewBag.ActiveMenu = "Registration"; //The menu item will be active for this page.
    ViewBag.Title = "Candidates";
}
<div class="widget">
    <div class="widget-head">
        <div class="pull-left">Selected Dealers</div>
        <div class="widget-icons pull-right">
            <a href="#" class="wminimize"><i class="icon-chevron-up"></i></a>
            <a href="#" class="wclose"><i class="icon-remove"></i></a>
        </div>
        <div class="clearfix"></div>
    </div>
@using (Html.BeginForm("Create", "Candidates", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="widget-content">
        <div class="padd">   
                <div class="form-horizontal">
                    
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })


                    <div class="form-group">
                        @Html.Label("Select Candidates Sheet", htmlAttributes: new { @class = "control-label col-md-2" })
                        <div class="col-md-8">
                            <input class="form-control" type="file" id="file" name="file" />

                        </div>
                       
                    </div>
                </div>

        </div>
        <div class="widget-foot">
            <a href="@Url.Action("Index")" class="btn btn-default">Back</a>
             <input type="submit" value="Upload" class="pull-right btn btn-primary" />
            <div class="clearfix"></div>
        </div>
</div>
}
    </div>